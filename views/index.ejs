<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel='stylesheet' type='text/css' media='screen' href='estilo/estilo.css'>
    <title>Cadastrar</title>
</head>
<body>
    <a href='/pessoas/criar'>Cadastrar alunos</a>
    <div id='cadastrar'>
    <% pessoas.forEach((pessoa) => { %>
       <div> <%= pessoa.nome %> </div>
       <div> <%= pessoa.sobrenome %> </div>
       <div> <%= pessoa.idade %> </div>
       <div> <%= pessoa.altura %> </div>
       <a href="/pessoas/deletar?id=<%=pessoa.id%>">Deletar</a>
    <% }) %>
</div>
</body>
</html>